extends ../layout/index

block content
    main.container.mt-5
        h1.text-center #{pageTitle}
        .row.upcoming-travels
            each travel in travels
                .col-md-6.col-lg-4.mb-4
                    .card
                        img.img-fluid.card-img-top(src=`/img/destiny_${travel.image}.jpg`)
                        .card-body
                            .card-body
                               h2 #{travel.title}
                